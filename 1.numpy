    "#create your own 1D array\n",
    "def make_array(start_value=0, end_value=10, seperate_by=2):# start and end values default to 0 and 10(excluded)\n",
    "    print(np.arange(start_value, end_value, seperate_by))\n",
    "    \n",
    "make_array(4,80,5)\n",
    "make_array(12,18,1.5)"
